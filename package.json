{
  "name": "figma-to-elementor-mcp",
  "version": "0.1.0",
  "description": "MCP server to convert Figma designs to Elementor v4 atomic widgets",
  "main": "dist/index.js",
  "bin": {
    "figma-to-elementor-mcp": "./bin/figma-to-elementor-mcp"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "figma",
    "elementor",
    "mcp",
    "model-context-protocol",
    "wordpress",
    "design-to-code",
    "atomic-widgets"
  ],
  "author": "Figma to Elementor MCP",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "axios": "^1.6.0",
    "commander": "^11.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^5.1.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.4.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/figma-to-elementor-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/figma-to-elementor-mcp/issues"
  },
  "homepage": "https://github.com/your-username/figma-to-elementor-mcp#readme"
}
